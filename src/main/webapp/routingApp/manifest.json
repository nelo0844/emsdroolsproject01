{  
   "_build":"BUILD_NUMBER",
   "_revision":"COMMIT_ID",
   "_version":"0.1.0",
   "sap.app":{  
      "_version":"0.1.0",
      "id":"sap.gm",
      "type":"application",
      "i18n":"i18n/i18n.properties",
      "title":"{{appTitle}}",
      "description":"{{appDescription}}",
      "applicationVersion":{  
         "version":"1.0.0"
      }
   },
   "sap.ui":{  
      "_version":"0.1.0",
      "technology":"UI5",
      "icons":{  
         "icon":"",
         "favIcon":"",
         "phone":"",
         "phone@2":"",
         "tablet":"",
         "tablet@2":""
      },
      "deviceTypes":{  
         "desktop":true,
         "tablet":true,
         "phone":false
      },
      "supportedThemes":[  
         "sap_bluecrystal"
      ]
   },
   "sap.ui5":{  
      "_version":"1.1.0",
      "rootView":{  
         "viewName":"sap.gm.view.App",
         "type":"XML",
         "id":"app"
      },
      "resources": {
            "css": [{
                "uri": "css/component.css",
                "id": "componentcss"
            }],
            "js":[{
					"uri": "js/Utils.js"
			}]
        },
      "dependencies":{  
         "minUI5Version":"1.38.16",
         "libs":{  
            "sap.ui.core":{  

            },
            "sap.m":{  

            },
            "sap.ui.layout":{  

            }
         }
      },
      "contentDensities":{  
         "compact":true,
         "cozy":true
      },
      "models":{  
         "i18n":{  
            "type":"sap.ui.model.resource.ResourceModel",
            "settings":{  
               "bundleName":"sap.gm.i18n.i18n"
            }
         }
      },
      "routing":{  
         "config":{  
            "routerClass":"sap.m.routing.Router",
            "viewType":"XML",
            "viewPath":"sap.gm.view",
            "controlId":"rootControl"
         },
         "routes":[  
            {  
               "pattern":"",
               "name":"master",
               "target":[  
                  "master"
               ]
            },
            {  
               "pattern":"main",
               "name":"mainPage",
               "target":[  
                  "master",
                  "mainPage"
               ]
            },
            {  
               "pattern":":ruleId:/detail",
               "name":"ruleDetail",
               "target":[  
                  "master",
                  "ruleDetail"
               ]
            },
            {  
               "pattern":"create/:ruleName:",
               "name":"ruleCreate",
               "target":[  
                  "master",
                  "ruleEdit"
               ]
            },
            {  
               "pattern":":ruleId:/edit",
               "name":"ruleEdit",
               "target":[  
                  "master",
                  "ruleEdit"
               ]
            }
         ],
         "targets":{  
            "master":{  
               "viewName":"Master",
               "controlAggregation":"masterPages",
               "viewLevel":0
            },
            "mainPage":{  
               "viewName":"Main",
               "controlAggregation":"detailPages",
               "viewLevel":1,
                "title": "EMS Rule Design"
            },
            "ruleDetail":{  
               "viewName":"Detail",
               "controlAggregation":"detailPages",
               "viewLevel":1,
               "title": "EMS Rule Detail"
            },
            "ruleEdit":{  
               "viewName":"Edit",
               "controlAggregation":"detailPages",
               "viewLevel":2,
                "title": "Edit EMS Rule"
            }
         }
      }
   }
}