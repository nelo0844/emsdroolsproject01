{  
   "_build":"BUILD_NUMBER",
   "_revision":"COMMIT_ID",
   "_version":"0.1.0",
   "sap.app":{  
      "_version":"0.1.0",
      "id":"sap.gm",
      "type":"application",
      "i18n":"i18n/i18n.properties",
      "title":"{{appTitle}}",
      "description":"{{appDescription}}",
      "applicationVersion":{  
         "version":"1.0.0"
      }
   },
   "sap.ui":{  
      "_version":"0.1.0",
      "technology":"UI5",
      "icons":{  
         "icon":"",
         "favIcon":"",
         "phone":"",
         "phone@2":"",
         "tablet":"",
         "tablet@2":""
      },
      "deviceTypes":{  
         "desktop":true,
         "tablet":true,
         "phone":false
      },
      "supportedThemes":[  
         "sap_bluecrystal"
      ]
   },
   "sap.ui5":{  
      "_version":"1.1.0",
      "rootView":{  
         "viewName":"sap.gm.view.App",
         "type":"XML",
         "id":"app"
      },
      "resources": {
            "css": [{
                "uri": "css/component.css",
                "id": "componentcss"
            }]
        },
      "dependencies":{  
         "minUI5Version":"1.38.16",
         "libs":{  
            "sap.ui.core":{  

            },
            "sap.m":{  

            },
            "sap.ui.layout":{  

            }
         }
      },
      "contentDensities":{  
         "compact":true,
         "cozy":true
      },
      "models":{  
         "i18n":{  
            "type":"sap.ui.model.resource.ResourceModel",
            "settings":{  
               "bundleName":"sap.gm.i18n.i18n"
            }
         }
      },
      "routing":{  
         "config":{  
            "routerClass":"sap.m.routing.Router",
            "viewType":"XML",
            "viewPath":"sap.gm.view",
            "controlId":"rootControl"
         },
         "routes":[  
            {  
               "pattern":"",
               "name":"master",
               "target":[  
                  "master"
               ]
            },
            {  
               "pattern":"orders/:orderId:",
               "name":"orderDetails",
               "target":[  
                  "master",
                  "orderDetails"
               ]
            },
            {  
               "pattern":"rules/:rulesId:",
               "name":"productDetails",
               "target":[  
                  "master",
                  "productDetails"
               ]
            }
         ],
         "targets":{  
            "master":{  
               "viewName":"Master",
               "controlAggregation":"masterPages",
               "viewLevel":0
            },
            "orderDetails":{  
               "viewName":"Detail",
               "controlAggregation":"detailPages",
               "title":{  
                  "parts":[  
                     "orderName"
                  ],
                  "formatter":"jQuery.sap.formatMessage"
               },
               "viewLevel":1
            },
            "productDetails":{  
               "viewName":"Edit",
               "controlAggregation":"detailPages",
               "title":{  
                  "parts":[  
                     "productName"
                  ],
                  "formatter":"jQuery.sap.formatMessage"
               },
               "viewLevel":2
            }
         }
      }
   }
}