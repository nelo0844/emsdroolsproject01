<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<MessagePopover
		title="Notification"
		items="{notificationModel>/data}"
		class="sapUiContentPadding">
		  <MessagePopoverItem type="Error" 	title= "Error message"
				subtitle="{notificationModel>message}" description="{notificationModel>message}"></MessagePopoverItem>
	</MessagePopover>
</core:FragmentDefinition>